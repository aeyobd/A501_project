&star_job

! basic controls
    create_pre_main_sequence_model = .true.

    change_net = .true.      
    new_net_name = 'sagb_NeNa_MgAl.net'

    ! use Asplund 09 abundances
    initial_zfracs = 8 


/ ! end of star_job namelist



&eos

/ ! end eos


&kap
    Zbase = 0.01 ! metallicity for kappa
    use_Type2_opacities = .true.

/ !end of kap namelist




&controls

! initial controls
    initial_mass = 2.000
    initial_z = 0.0100

    energy_eqn_option = 'dedt'


! output
    photo_interval = 50                                                    
    profile_interval = 100                                                 
    history_interval = 10                                                  
    terminal_interval = 10                                                 
    write_header_frequency = 10                                            



! mass loss       
    cool_wind_RGB_scheme = 'Reimers' 
    Reimers_scaling_factor = 0.5 
    cool_wind_AGB_scheme = 'Blocker' 
    Blocker_scaling_factor = 0.04
    RGB_to_AGB_wind_switch = 1d-4
    cool_wind_full_on_T = 9.99d9
    hot_wind_full_on_T = 1d10


! simple atmosphere
    atm_option = 'T_tau'
    atm_T_tau_relation = 'Eddington'
    atm_T_tau_opacity = 'fixed'


! convection and overshooting
    mixing_length_alpha = 1.73

    overshoot_scheme(1) = 'exponential'
    overshoot_zone_type(1) = 'any'
    overshoot_zone_loc(1) = 'any'
    overshoot_bdy_loc(1) = 'any'
    overshoot_f(1) = 0.014
    overshoot_f0(1) = 0.004


! timestepping limits
    varcontrol_target = 1d-4

/ ! end of controls namelist


