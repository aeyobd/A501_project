&star_job

! basic controls
    create_pre_main_sequence_model = .true.

    change_net = .true.      
    new_net_name = 'sagb_NeNa_MgAl.net'
    adjust_abundances_for_new_isos = .true.

    change_D_omega_flag = .true.
    new_D_omega_flag = .true.

/ ! end of star_job namelist



&eos

/ ! end eos


&kap
    Zbase = 0.01 ! metallicity for kappa
    use_Type2_opacities = .true.
    kap_file_prefix = 'gs98'

/ !end of kap namelist


&atm
! simple atmosphere
    atm_option = 'T_tau'
    atm_T_tau_relation = 'Eddington'
    atm_T_tau_opacity = 'fixed'
/ ! end of atm namelist



&controls

! initial controls
    initial_mass = 1.000
    initial_z = 0.01

    energy_eqn_option = 'eps_grav'
    varcontrol_target = 1d-3
    use_gold2_tolerances = .true.

    convergence_ignore_equL_residuals = .true. ! for FLAST??
    delta_lgL_He_limit = 0.01

    ! mixing_length_alpha = 1.73


! output
    photo_interval = 50                                                    
    profile_interval = 10                                                 
    max_num_profile_models = 1000
    history_interval = 1                                                  
    terminal_interval = 10                                                 
    write_header_frequency = 10                                            

    num_trace_history_values = 2
    trace_history_value_name(1) = 'rel_E_err'
    trace_history_value_name(2) = 'log_rel_run_E_err'


! mass loss       
    cool_wind_RGB_scheme = 'Reimers' 
    Reimers_scaling_factor = 0.5d0
    cool_wind_AGB_scheme = 'Blocker' 
    Blocker_scaling_factor = 0.1d0  ! could be as los as 0.0003
    RGB_to_AGB_wind_switch = 1d-4
    cool_wind_full_on_T = 9.99d9
    hot_wind_full_on_T = 1d10


! angular momentum diffusion
  am_nu_visc_factor = 0
  am_D_mix_factor = 0.0333333333333333d0
  D_DSI_factor = 0
  D_SH_factor = 1
  D_SSI_factor = 1
  D_ES_factor = 1
  D_GSF_factor = 1
  D_ST_factor = 1

/ ! end of controls namelist


